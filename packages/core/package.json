{
  "name": "@zodui/core",
  "version": "0.2.0-alpha.1",
  "license": "MIT",
  "main": "dist/esm/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "typesVersions": {
    "*": { "*": [ "dist/dts/*" ] }
  },
  "exports": {
    ".": {
      "inner-src": "./src/index.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/esm/index.js"
    },
    "./utils": {
      "inner-src": "./src/utils.ts",
      "import": "./dist/esm/utils.js",
      "require": "./dist/cjs/utils.js"
    },
    "./external": {
      "inner-src": "./src/external.ts",
      "import": "./dist/esm/external.js",
      "require": "./dist/cjs/external.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/cjs",
    "dist/esm",
    "dist/dts",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c && yarn build:dts",
    "build:dts": "tsc -p tsconfig.dts.json --outDir dist/dts"
  },
  "dependencies": {
    "zod": "^3.21.4"
  }
}
